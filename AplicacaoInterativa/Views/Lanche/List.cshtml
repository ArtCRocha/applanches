@model LancheListViewModel

<div class="container center">
    <h2 class="text-center" >@ViewData["Titulo"]</h2>
    <h2 class="text-center" >@Model.CategoriaAtual</h2>
</div>

<div class="container" style="align-items: center;">
    <div class="row">
      @foreach (var lanche in Model.Lanches){
       <partial name="_LanchesResumo" for="@lanche"/>
        }
    </div>
</div>