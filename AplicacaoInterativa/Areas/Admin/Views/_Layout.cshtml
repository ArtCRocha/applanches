<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
      <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min-alternativo.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/AplicacaoInterativa.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/fontawesome-free-6.1.1-web/css/all.css" asp-append-version="true" />
   
</head>
<body>
     <header class="color-background">
       <div class="container">
        <nav class=" navbar navbar-expand-lg navbar-dark">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="~/images/AppLanches_logo-removebg-preview.png"/></a>
         <div class="navbar-collapse collapse d-sm-inline-flex">
                    <a class="nav-link text-light" asp-area="Admin" asp-controller="Admin" asp-action="Index">Home</a>
                    <a class="nav-link text-light" asp-area="Admin" asp-controller="AdminLanches" asp-action="Index">Lanches</a>
                    <a class="nav-link text-light" asp-area="Admin" asp-controller="AdminCategorias" asp-action="Index">Categorias</a>
                    <a class="nav-link text-light" asp-area="Admin" asp-controller="AdminPedidos" asp-action="Index">Pedidos</a>
                     <a class="nav-link text-light" asp-area="Admin" asp-controller="AdminImagens" asp-action="Index">Gerenciar Imagens</a>
          </div>
         </nav>
        </div>
    </header>
    <div>
        @RenderBody()
    </div>
     <footer class="color-background">
    <div class="container d-flex flex-wrap justify-content-around align-items-center py-3 my-4 ">
        <p class="col-md-4 mb-0 text-muted">Projeto Acadêmico - 2022 </p>

    <a href="/" class="col-md-4 d-flex align-items-center justify-content-center mb-3 mb-md-0 me-md-auto link-dark text-decoration-none">
     <img src="~/images/AppLanches_logo-removebg-preview.png"/>
    </a>

    <ul class="nav col-md-4 justify-content-end">
       
                <li class="nav-item">
                  <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Privacy">Privacidade</a>
                </li>
                
    </ul>
    </div>
  </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
